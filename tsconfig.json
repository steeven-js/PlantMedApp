{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "tests": ["tests/*"],
      "@src/*": ["src/*"],
      "@constants/*": ["src/constants/*"],
      "@custom/*": ["src/custom/*"],
      "@data/*": ["src/data/*"],
      "@hooks/*": ["src/hooks/*"],
      "@navigation/*": ["src/navigation/*"],
      "@screens/*": ["src/screens/*"],
      "@store/*": ["src/store/*"],
      "@text/*": ["src/text/*"],
      "@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"],
      "@components/*": ["src/components/*"],
    }
  }
}